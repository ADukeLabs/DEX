@using DEX.ViewModels
@model DEX.ViewModels.CityViewModel


    <div class="row">
       <div class="content-box col-xs-12 col-md-10 col-md-offset-1">
            @foreach (var c in Model.Cities)
            {
                <div id="@c.Id" class="city-div">
                    <button type="button" class="GetCompanies btn btn-primary btn-block" data-toggle="collapse" data-target="company-div">@c.Name</button>
                </div>
            }
       </div>
    </div>
                                                                    