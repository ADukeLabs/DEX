@using DEX.ViewModels
@model DEX.ViewModels.CityViewModel

<div class="container-fluid">
    <div class="row">
        <div class="well col-xs-8 col-md-offset-2">
            <div class="btn-group-vertical">
                @foreach (var c in Model.Cities)
                {
                    <button class="btn btn-default col-xs-12">@c.Name</button>
                }
            </div>
        </div>
    </div>
</div>



                                                                          